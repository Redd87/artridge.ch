function updatemenu()
 for i=1,4 do
  if btnp(i-1) then
   menuci+=dirx[i]
   menuci+=diry[i]*8
  end
 end
 menuci=mid(1,menuci,menulvls)
 
 if btnp(âŽ) then
  resetlvl()
 end
end

function updategame()
 if btnp(ðŸ…¾ï¸) then
  _upd=updatemenu
  _drw=drawmenu
  resetarrows()
 end
 
 local x=flr(px/8)+mapx
 local y=flr(py/8)+mapy
 local s=mget(x,y)
 
 if fget(s,2) then
  pv=-1
 elseif fget(s,1) then
 	resetarrows()
  resetlvl()
 end

 
 if s==4 then
  _upd=updatemenu
  _drw=drawmenu
  store(lvl,1)
  resetarrows()
 elseif s==5 then
 	resetarrows()
  resetlvl()
  store(lvl+32,1)
 end
 
 local spikes={24,25,41,40}
 local arrows={103,102,118,119}
 for i=1,4 do
  if pv==i-1 and s==spikes[i] then
   resetarrows()
   resetlvl()
  end
  if s==arrows[i] then
   pv=i-1
   movedist=0
   if fget(mget(x+dirx[pv+1],y+diry[pv+1]),0) then
   	stop()
   end
  end
 end
 
 if pv==-1 then
  for i=1,4 do
   if btnp(i-1) then
    pv=i-1
    if not fget(mget(x+dirx[i],y+diry[i]),0) then
    	isblue=not isblue
    end
    for x=mapx,mapx+16 do
     for y=mapy,mapy+16 do
      if isblue then
      	if (mget(x,y)==22) mset(x,y,21)
      	if (mget(x,y)==19) mset(x,y,20)
      else
       if (mget(x,y)==21) mset(x,y,22)
      	if (mget(x,y)==20) mset(x,y,19)
      end
     end
    end
   end
  end
 end
 
 for num in all(props.num) do
  if pv!=-1 and movedist>0 and not props.numdone  then
	 	if px+dirx[pv+1]*8==num.c.x and py+diry[pv+1]*8==num.c.y then
	 	 if props.numc+1==num.n then
	 	 	props.numc+=1
	 	 	num.unlocked=true
	 	 	if count(props.num)==props.numc then
	 	 		mset(props.numend.x,props.numend.y,42)
	 	 		props.numdone=true
	 	 	end
	 	 else
	 	 	props.numc=0
	 	 	for num in all(props.num) do
	 	 	 num.unlocked=false
	 	 	end
	 	 end
	 	end
	 end
 end
 
 for i=1,4 do
  if pv==i-1 then
   if mget(x+dirx[i],y+diry[i])==10 and movedist>=3 then
    mset(x+dirx[i],y+diry[i],26)
   end
   if fget(mget(x+dirx[i],y+diry[i]),0) then
   	pv=-1
   	movedist=0
	  else
	   px+=dirx[i]*8
	   py+=diry[i]*8
	   movedist+=1
	  end
  end
 end
 
 for bt in all(props.arrbut) do
  if px==bt.x and py==bt.y and pv==bt.v then
  	rotatearrows()
  end
 end

 for i,tp in pairs(props.wtp) do
 	if px==tp[1].x and py==tp[1].y and pv==tp[1].v then
 	 px,py=tp[2].x,tp[2].y,tp[1].v
 	 break
 	elseif px==tp[2].x and py==tp[2].y and pv==tp[2].v then
 	 px,py,pv=tp[1].x,tp[1].y,tp[2].v
 	 break
 	end
 end
 
 tped=true
 if btnp(âŽ) then
  for i,t in pairs(props.ptp) do
   tp(i,t,props.ptp)
  end
  for i,t in pairs(props.btp) do
   tp(i,t,props.btp)
  end
  for i,t in pairs(props.rtp) do
   tp(i,t,props.rtp)
  end
  for i,t in pairs(props.gtp) do
   tp(i,t,props.gtp)
  end
  for i,t in pairs(props.ytp) do
   tp(i,t,props.ytp)
  end
 end
end

function tp(i,t,arr)
 if px==t.x and py==t.y and tped then
  px=arr[i==1 and 2 or 1].x
  py=arr[i==1 and 2 or 1].y
  tped=false
 end
end

function rotatearrows()
	local arrspr={102,119,103,118}
	for arr in all(arrows) do
		mset(arr.x,arr.y,
			arrspr[
				(indexof(
					arrspr,mget(arr.x,arr.y)
				))%4+1
			]
		)
	end
end

function store(i,val)
	poke(0x5f80+i,val)
end

function get_data(i)
 return peek(0x5f80+i)
end
