function _init()
 menuitem(1,"clear data",function() for i=1,63 do store(i,0) end end)
 -- main functions
 _upd=updatemenu
 _drw=drawmenu
 
 -- player
 pv=-1
 
 -- menu
 menuci=1
 menulvls=0
 
 -- misc
 debug={}
 dirx,diry={-1,1,0,0},{0,0,-1,1}
end

function _update()
 mapx=((menuci-1)%8)*16
 mapy=flr((menuci-1)/8)*16
 
 _upd()
end

function _draw()
 _drw()
 
 cursor(8,8,8)
 for d in all(debug) do
  print(d)
 end
 
