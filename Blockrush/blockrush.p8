function drawmenu()
 cls()
 local i=1
 
 menulvls=0
 for _y=0,3 do
  for _x=0,7 do
   local x,y,found=_x*10,_y*8,false
   local mapx=((i-1)%8)*16
 		local mapy=(flr(i/(i%8==0 and 9 or 8)))*16
   for x2=mapx,mapx+15 do
    for y2=mapy,mapy+16 do
     found=mget(x2,y2)==16 and true or found
    end
   end
   if found then
	   print(i,x+23,y+40,found and 7 or 8)
	   if (i==menuci) rect(x+21,y+38,x+31,y+46,7)
	   if (get_data(i)==1) spr(34,x+28,y+42)
	   if (get_data(i+32)==1) spr(50,x+28,y+38)
   	menulvls+=1
   end
   i+=1
  end
 end
end

function drawgame()
 cls(5)
 palt(0,false)
 map(mapx,mapy)
 pal()
 spr(3,props.start.x,props.start.y)
 if (get_data(lvl+32)==1) spr(17,props.crown.x,props.crown.y)
 spr(1,px,py)
 
 for b in all(props.num) do
 	if (b.unlocked) pal(7,11)
 	spr(b.s,b.c.x,b.c.y)
 	pal()
 end
end
