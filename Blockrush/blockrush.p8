function assert(cond,msg)
 if not cond then
	 cursor(1,1,8)
	 print("runtime error:")
	 color(6)
  stop(msg)
 end
end

function indexof(arr,el)
	for k,v in pairs(arr) do
		if (v==el) return k
	end
	return -1
end
